Q138: Print all enum names and integer values using a loop.
#include <stdio.h>

enum Items {
    ITEM1 = 1,
    ITEM2,
    ITEM3,
    ITEM4,
    ITEM5
};

int main() {
    enum Items i;

    for (i = ITEM1; i <= ITEM5; i++) {
        switch (i) {
            case ITEM1: printf("ITEM1 = %d\n", i); break;
            case ITEM2: printf("ITEM2 = %d\n", i); break;
            case ITEM3: printf("ITEM3 = %d\n", i); break;
            case ITEM4: printf("ITEM4 = %d\n", i); break;
            case ITEM5: printf("ITEM5 = %d\n", i); break;
        }
    }

    return 0;
}
